{
  "name": "@gulitsky/solidity-starter",
  "bugs": "https://github.com/gulitsky/solidity-starter",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@defi-wonderland/smock": "^2.3.4",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.11",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.8.3",
    "@openzeppelin/contracts-upgradeable": "^4.8.3",
    "@swc/core": "^1.2.101",
    "@swc/helpers": "^0.2.13",
    "@swc/register": "^0.1.7",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.0",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.6",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.9.4",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-packager": "^1.4.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.3.0",
    "tslib": "^2.3.1",
    "typechain": "^5.2.0",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.5"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "license": "ISC",
  "private": true,
  "repository": "github:gulitsky/solidity-starter",
  "scripts": {
    "build": "hardhat compile",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp ./artifacts/ --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat deploy",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"./contracts/**/*.sol\"",
    "lint": "yarn run lint:sol && yarn prettier",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prettier": "prettier --check --config ./.prettierrc  \"./**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"./**/*.{js,json,md,sol,ts,yml}\"",
    "rebuild": "yarn run build --force",
    "test": "hardhat test",
    "typechain": "hardhat typechain",
    "verify": "hardhat etherscan-verify --solc-input"
  },
  "packageManager": "yarn@3.5.0"
}
