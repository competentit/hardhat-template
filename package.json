{
  "name": "@gulitsky/solidity-starter",
  "bugs": "https://github.com/gulitsky/solidity-starter",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@defi-wonderland/smock": "^2.3.4",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@openzeppelin/contracts": "^4.8.3",
    "@openzeppelin/contracts-upgradeable": "^4.8.3",
    "@swc/core": "^1.3.55",
    "@swc/helpers": "^0.5.1",
    "@swc/register": "^0.1.10",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.1",
    "chai": "^4.3.7",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "fs-extra": "^11.1.1",
    "hardhat": "^2.14.0",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-deploy": "^0.11.26",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-packager": "^1.4.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typechain": "^8.1.1",
    "typescript": "~5.0.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.5"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "license": "ISC",
  "private": true,
  "repository": "github:gulitsky/solidity-starter",
  "scripts": {
    "build": "hardhat compile",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp ./artifacts/ --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat deploy",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"./contracts/**/*.sol\"",
    "lint": "yarn run lint:sol && yarn prettier",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prettier": "prettier --check --config ./.prettierrc  \"./**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"./**/*.{js,json,md,sol,ts,yml}\"",
    "rebuild": "yarn run build --force",
    "test": "hardhat test",
    "typechain": "hardhat typechain",
    "verify": "hardhat etherscan-verify --solc-input"
  },
  "packageManager": "yarn@3.5.0"
}
